<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xmsy.server.zxyy.webhome.modules.manager.webhomehead.dao.WebhomeHeadDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.xmsy.server.zxyy.webhome.modules.manager.webhomehead.entity.WebhomeHeadEntity" id="webhomeHeadMap">
        <result property="id" column="id"/>
        <result property="deleteStatus" column="delete_status"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
        <result property="version" column="version"/>
        <result property="name" column="name"/>
        <result property="url" column="url"/>
        <result property="content" column="content"/>
        <result property="enclosureId" column="enclosure_id"/>
        <result property="type" column="type"/>
    </resultMap>
    
    <select id="selectListByWeb" resultType="com.xmsy.server.zxyy.webhome.modules.manager.webhomehead.entity.WebhomeHeadEntity">
		SELECT
		a.type as type
	FROM
		webhome_head a
		where a.delete_status!=1
	GROUP BY
		a.type
	</select>
	<select id="selectListBytype" resultType="com.xmsy.server.zxyy.webhome.modules.manager.webhomehead.entity.WebhomeHeadEntity">
		SELECT
			a.content,
			a.url,
			a.type,
			b.url AS enclosureUrl
		FROM
			webhome_head a
		LEFT JOIN webhome_enclosure b ON a.enclosure_id = b.id
		WHERE
		a.delete_status!=1
		and
			a.type = #{type}
	</select>
    
    
    
</mapper>